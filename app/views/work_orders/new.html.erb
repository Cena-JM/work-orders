<h1>Create Work Order</h1>

<%= form_for(@work_order) do |f| %>

<%= f.label :title %><br>
<%= f.text_field :title %><br>

<%= f.label :description %><br>
<%= f.text_field :description %><br>

<%= f.label :deadline %><br>
<%= f.date_field :deadline %><br>

<%= f.submit "Create my work order" %>
<% end %>